      </itext>
      <instance>
        <training_opinion_form_translated id="training_opinion_form_translated" version="9">
          <start_time/>
          <end_time/>
          <today_date/>
          <imei/>
          <imsi/>
          <sim/>
          <personal>
            <name/>
            <age/>
            <profession/>
          </personal>
          <opinion>
            <food_preferences/>
            <brew_preference/>
          </opinion>
          <coffee_preferences>
            <sugar>4.2</sugar>
            <style/>
            <amount>2</amount>
          </coffee_preferences>
          <tea_preferences>
            <last_coffee/>
          </tea_preferences>
          <risk/>
          <perc_sugar/>
          <sugar_note/>
          <ack_cookies/>
          <pic/>
          <ack/>
          <meta>
            <instanceID/>
          </meta>
        </training_opinion_form_translated>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/training_opinion_form_translated/start_time" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/training_opinion_form_translated/end_time" type="dateTime"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/training_opinion_form_translated/today_date" type="date"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/training_opinion_form_translated/imei" type="string"/>
      <bind jr:preload="property" jr:preloadParams="subscriberid" nodeset="/training_opinion_form_translated/imsi" type="string"/>
      <bind jr:preload="property" jr:preloadParams="simserial" nodeset="/training_opinion_form_translated/sim" type="string"/>
      <bind nodeset="/training_opinion_form_translated/personal/name" type="string"/>
      <bind nodeset="/training_opinion_form_translated/personal/age" required="true()" type="select1"/>
      <bind nodeset="/training_opinion_form_translated/personal/profession" type="select1"/>
      <bind nodeset="/training_opinion_form_translated/opinion/food_preferences" type="select"/>
      <bind nodeset="/training_opinion_form_translated/opinion/brew_preference" required="true()" type="select1"/>
      <bind nodeset="/training_opinion_form_translated/coffee_preferences" relevant=" /training_opinion_form_translated/opinion/brew_preference =&quot;coffee&quot;"/>
      <bind nodeset="/training_opinion_form_translated/coffee_preferences/sugar" required="true()" type="decimal"/>
      <bind nodeset="/training_opinion_form_translated/coffee_preferences/style" required="true()" type="string"/>
      <bind nodeset="/training_opinion_form_translated/coffee_preferences/amount" required="true()" type="int"/>
      <bind nodeset="/training_opinion_form_translated/tea_preferences" relevant=" /training_opinion_form_translated/opinion/brew_preference =&quot;tea&quot;"/>
      <bind nodeset="/training_opinion_form_translated/tea_preferences/last_coffee" type="date"/>
      <bind calculate=" /training_opinion_form_translated/coffee_preferences/sugar * /training_opinion_form_translated/coffee_preferences/amount " nodeset="/training_opinion_form_translated/risk" type="string"/>
      <bind calculate=" /training_opinion_form_translated/risk  * 4" nodeset="/training_opinion_form_translated/perc_sugar" type="string"/>
      <bind nodeset="/training_opinion_form_translated/sugar_note" readonly="true()" relevant=" /training_opinion_form_translated/opinion/brew_preference =&quot;coffee&quot;" type="string"/>
      <bind nodeset="/training_opinion_form_translated/ack_cookies" relevant="not(selected( /training_opinion_form_translated/opinion/food_preferences ,&quot;cookies&quot;))" type="string"/>
      <bind nodeset="/training_opinion_form_translated/pic" type="binary"/>
      <bind nodeset="/training_opinion_form_translated/ack" required="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/training_opinion_form_translated/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="field-list" ref="/training_opinion_form_translated/personal">
      <label ref="jr:itext('/training_opinion_form_translated/personal:label')"/>
      <input ref="/training_opinion_form_translated/personal/name">
        <label ref="jr:itext('/training_opinion_form_translated/personal/name:label')"/>
      </input>
      <select1 ref="/training_opinion_form_translated/personal/age">
        <label ref="jr:itext('/training_opinion_form_translated/personal/age:label')"/>
        <item>
          <label ref="jr:itext('/training_opinion_form_translated/personal/age/under30:label')"/>
          <value>under30</value>
        </item>
        <item>
          <label ref="jr:itext('/training_opinion_form_translated/personal/age/over30:label')"/>
          <value>over30</value>
        </item>
      </select1>
      <select1 ref="/training_opinion_form_translated/personal/profession">
        <label ref="jr:itext('/training_opinion_form_translated/personal/profession:label')"/>
        <item>
          <label ref="jr:itext('/training_opinion_form_translated/personal/profession/programmer:label')"/>
          <value>programmer</value>
        </item>
        <item>
          <label ref="jr:itext('/training_opinion_form_translated/personal/profession/sysadmin:label')"/>
          <value>sysadmin</value>
        </item>
      </select1>
    </group>
    <group appearance="field-list" ref="/training_opinion_form_translated/opinion">
