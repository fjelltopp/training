        <item>
          <label ref="jr:itext('/training_opinion_form_translated/opinion/food_preferences/cookies:label')"/>
          <value>cookies</value>
        </item>
        <item>
          <label ref="jr:itext('/training_opinion_form_translated/opinion/food_preferences/dates:label')"/>
          <value>dates</value>
        </item>
        <item>
          <label ref="jr:itext('/training_opinion_form_translated/opinion/food_preferences/falafel:label')"/>
          <value>falafel</value>
        </item>
        <item>
          <label ref="jr:itext('/training_opinion_form_translated/opinion/food_preferences/shawarma:label')"/>
          <value>shawarma</value>
        </item>
        <item>
          <label ref="jr:itext('/training_opinion_form_translated/opinion/food_preferences/pizza:label')"/>
          <value>pizza</value>
        </item>
        <item>
          <label ref="jr:itext('/training_opinion_form_translated/opinion/food_preferences/cheese:label')"/>
          <value>cheese</value>
        </item>
      </select>
      <select1 ref="/training_opinion_form_translated/opinion/brew_preference">
        <label ref="jr:itext('/training_opinion_form_translated/opinion/brew_preference:label')"/>
        <item>
          <label ref="jr:itext('/training_opinion_form_translated/opinion/brew_preference/coffee:label')"/>
          <value>coffee</value>
        </item>
        <item>
          <label ref="jr:itext('/training_opinion_form_translated/opinion/brew_preference/tea:label')"/>
          <value>tea</value>
        </item>
      </select1>
    </group>
    <group appearance="field-list" ref="/training_opinion_form_translated/coffee_preferences">
      <label ref="jr:itext('/training_opinion_form_translated/coffee_preferences:label')"/>
      <input ref="/training_opinion_form_translated/coffee_preferences/sugar">
        <label ref="jr:itext('/training_opinion_form_translated/coffee_preferences/sugar:label')"/>
      </input>
      <input ref="/training_opinion_form_translated/coffee_preferences/style">
        <label ref="jr:itext('/training_opinion_form_translated/coffee_preferences/style:label')"/>
      </input>
      <input ref="/training_opinion_form_translated/coffee_preferences/amount">
        <label ref="jr:itext('/training_opinion_form_translated/coffee_preferences/amount:label')"/>
      </input>
    </group>
    <group appearance="field-list" ref="/training_opinion_form_translated/tea_preferences">
      <label ref="jr:itext('/training_opinion_form_translated/tea_preferences:label')"/>
      <input ref="/training_opinion_form_translated/tea_preferences/last_coffee">
        <label ref="jr:itext('/training_opinion_form_translated/tea_preferences/last_coffee:label')"/>
      </input>
    </group>
    <input ref="/training_opinion_form_translated/sugar_note">
      <label ref="jr:itext('/training_opinion_form_translated/sugar_note:label')"/>
    </input>
    <trigger ref="/training_opinion_form_translated/ack_cookies">
      <label ref="jr:itext('/training_opinion_form_translated/ack_cookies:label')"/>
    </trigger>
    <upload mediatype="image/*" ref="/training_opinion_form_translated/pic">
      <label ref="jr:itext('/training_opinion_form_translated/pic:label')"/>
    </upload>
    <trigger ref="/training_opinion_form_translated/ack">
      <label ref="jr:itext('/training_opinion_form_translated/ack:label')"/>
    </trigger>
  </h:body>
</h:html>
