<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema"><h:head><h:title>Training opinion form</h:title><model><instance><training_opinion_form id="training_opinion_form" version="9"><start_time/><end_time/><today_date/><imei/><imsi/><sim/><personal><name/><age/><profession/></personal><opinion><food_preferences/><brew_preference/></opinion><coffee_preferences><sugar>4.2</sugar><style/><amount>2</amount></coffee_preferences><tea_preferences><last_coffee/></tea_preferences><risk/><perc_sugar/><sugar_note/><ack_cookies/><pic/><ack/><meta><instanceID/></meta></training_opinion_form></instance><bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/training_opinion_form/start_time" type="dateTime"/><bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/training_opinion_form/end_time" type="dateTime"/><bind jr:preload="date" jr:preloadParams="today" nodeset="/training_opinion_form/today_date" type="date"/><bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/training_opinion_form/imei" type="string"/><bind jr:preload="property" jr:preloadParams="subscriberid" nodeset="/training_opinion_form/imsi" type="string"/><bind jr:preload="property" jr:preloadParams="simserial" nodeset="/training_opinion_form/sim" type="string"/><bind nodeset="/training_opinion_form/personal/name" type="string"/><bind nodeset="/training_opinion_form/personal/age" required="true()" type="select1"/><bind nodeset="/training_opinion_form/personal/profession" type="select1"/><bind nodeset="/training_opinion_form/opinion/food_preferences" type="select"/><bind nodeset="/training_opinion_form/opinion/brew_preference" required="true()" type="select1"/><bind nodeset="/training_opinion_form/coffee_preferences" relevant=" /training_opinion_form/opinion/brew_preference =&quot;coffee&quot;"/><bind nodeset="/training_opinion_form/coffee_preferences/sugar" required="true()" type="decimal"/><bind nodeset="/training_opinion_form/coffee_preferences/style" required="true()" type="string"/><bind nodeset="/training_opinion_form/coffee_preferences/amount" required="true()" type="int"/><bind nodeset="/training_opinion_form/tea_preferences" relevant=" /training_opinion_form/opinion/brew_preference =&quot;tea&quot;"/><bind nodeset="/training_opinion_form/tea_preferences/last_coffee" type="date"/><bind calculate=" /training_opinion_form/coffee_preferences/sugar * /training_opinion_form/coffee_preferences/amount " nodeset="/training_opinion_form/risk" type="string"/><bind calculate=" /training_opinion_form/risk  * 4" nodeset="/training_opinion_form/perc_sugar" type="string"/><bind nodeset="/training_opinion_form/sugar_note" readonly="true()" relevant=" /training_opinion_form/opinion/brew_preference =&quot;coffee&quot;" type="string"/><bind nodeset="/training_opinion_form/ack_cookies" relevant="not(selected( /training_opinion_form/opinion/food_preferences ,&quot;cookies&quot;))" type="string"/><bind nodeset="/training_opinion_form/pic" type="binary"/><bind nodeset="/training_opinion_form/ack" required="true()" type="string"/><bind calculate="concat('uuid:', uuid())" nodeset="/training_opinion_form/meta/instanceID" readonly="true()" type="string"/></model></h:head><h:body><group appearance="field-list" ref="/training_opinion_form/personal"><label>Personal Information</label><input ref="/training_opinion_form/personal/name"><label>What is your name?</label></input><select1 ref="/training_opinion_form/personal/age"><label>What is your age?</label><item><label>Under 30 years old</label><value>under30</value></item><item><label>Over 30 years old</label><value>over30</value></item></select1><select1 ref="/training_opinion_form/personal/profession"><label>What is your profession?</label><item><label>Programmer</label><value>programmer</value></item><item><label>System Administator</label><value>sysadmin</value></item></select1></group><group appearance="field-list" ref="/training_opinion_form/opinion"><label>Your Opinions</label><select ref="/training_opinion_form/opinion/food_preferences"><label>Please choose types of food which you like to **eat**.</label><item><label>Cookies</label><value>cookies</value></item><item><label>Dates</label><value>chocolate</value></item><item><label>Pizza</label><value>pizza</value></item><item><label>Cheese</label><value>cheese</value></item></select><select1 ref="/training_opinion_form/opinion/brew_preference"><label>Which brew do you prefer?</label><item><label>Coffee</label><value>coffee</value></item><item><label>Tea</label><value>tea</value></item></select1></group><group appearance="field-list" ref="/training_opinion_form/coffee_preferences"><label>Coffee Preferences</label><input ref="/training_opinion_form/coffee_preferences/sugar"><label>How many grams of sugar do you drink with each coffee (one teaspoon is 4.2 grams)?</label></input><input ref="/training_opinion_form/coffee_preferences/style"><label>What type of coffee do you prefer?</label></input><input ref="/training_opinion_form/coffee_preferences/amount"><label>How many times a day do you drink coffee?</label></input></group><group appearance="field-list" ref="/training_opinion_form/tea_preferences"><label>Tea Preferences</label><input ref="/training_opinion_form/tea_preferences/last_coffee"><label>We are not interested in tea! When was the last time you drank coffee?</label></input></group><input ref="/training_opinion_form/sugar_note"><label>According to WHO guidelines, you get <output value=" /training_opinion_form/perc_sugar "/> percent of your daily sugar intake from coffee!</label></input><trigger ref="/training_opinion_form/ack_cookies"><label>Do you acknowledge that you don't like delicious cookies? 
